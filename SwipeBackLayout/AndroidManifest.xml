<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.renyu.swipebacklayout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.renyu.swipebacklayout.SwipeNormalActivity" android:theme="@android:style/Theme.Translucent"></activity>
        <activity android:name="com.renyu.swipebacklayout.SwipeScrollActivity" android:theme="@android:style/Theme.Translucent"></activity>
        <activity android:name="com.renyu.swipebacklayout.SwipeListViewActivity" android:theme="@android:style/Theme.Translucent"></activity>
       	<activity android:name="com.renyu.swipebacklayout.SwipeViewPagerActivity" android:theme="@android:style/Theme.Translucent"></activity>
        
        </application>

</manifest>
